{
  "expo": {
    "name": "Bari Food",
    "slug": "bari-food",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/bari-lion.png",
    "scheme": "barifoodapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.barifood.app"
    },
    "android": {
      "package": "com.yourcompany.barifood",
      "versionCode": 1,
      "permissions": []
    },
    "updates": {
      "enabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/logo/bari-lion.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4cdb34b6-97b5-4f08-848f-6eb566496ed4"
      }
    }
  }
}

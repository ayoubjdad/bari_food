{
  "expo": {
    "name": "Bari Food",
    "slug": "bari-food",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/bari-lion.png",
    "scheme": "bari_food_app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bari-food.bari_food_app"
    },
    "android": {
      "package": "com.yourcompany.myapp"
    },
    "updates": {
      "enabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/logo/bari-lion.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
